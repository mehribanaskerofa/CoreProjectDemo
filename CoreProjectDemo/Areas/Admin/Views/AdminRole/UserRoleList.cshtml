@using EntityLayer.Concrete
@model List<AppUser>
@{
    ViewData["Title"] = "UserRoleList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>UserRoleList</h1>


<table class="table table-hover">
    <tr>
        <th>User</th>
        <th>add role</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/Admin/AdminRole/AssignRole/@item.Id/" class="btn btn-outline-danger">add</a></td>
        </tr>
    }
</table>
<a href="/Admin/AdminRole/Index" class="btn btn-primary">Role lists</a>